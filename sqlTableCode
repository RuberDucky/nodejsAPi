CREATE TABLE users (
  usercustomerId INT AUTO_INCREMENT PRIMARY KEY,
  one_signal_id VARCHAR(36) NOT NULL,
  cover_images_id INT NOT NULL,
  full_name VARCHAR(255),
  username VARCHAR(255),
  email VARCHAR(255) NOT NULL,
  password VARCHAR(255) NOT NULL,
  account_type VARCHAR(50) NOT NULL,
  profile_picture VARCHAR(255) NOT NULL,
  social_acc_type VARCHAR(50) NOT NULL,
  google_access_token VARCHAR(255) NOT NULL,
  facebook_id VARCHAR(255),
  date_added DATE,
  status VARCHAR(20) NOT NULL,
  otp VARCHAR(255) NOT NULL,
  notifications VARCHAR(3) NOT NULL,
  be_seen VARCHAR(3) NOT NULL,
  first_name VARCHAR(255) NOT NULL,
  middle_name VARCHAR(255),
  last_name VARCHAR(255) NOT NULL,
  phone_number VARCHAR(20),
  gender VARCHAR(10),
  nationality VARCHAR(100),
  dob DATE,
  number_of_pages INT,
  currency VARCHAR(10)
);



CREATE TABLE cover_design (
  cover_images_id INT AUTO_INCREMENT PRIMARY KEY,
  name VARCHAR(255) NOT NULL,
  image VARCHAR(255) NOT NULL
);
